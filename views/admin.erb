<div class="span14">
	<h2>Phone Models</h2>
	<table class="bordered-table zebra-striped">
		<thead>
			<tr>
				<th>#</th>
				<th>Make</th>
				<th>Model</th>
				<th>Price</th>
				<th>Created</th>
			</tr>
		</thead>
		<tbody>
		<% for phoneModel in @models %>
			<tr>
				<td><%= phoneModel.id %></td>
				<td><%= phoneModel.make %></td>
				<td><%= phoneModel.phone_model %></td>
				<td><%= phoneModel.price %></td>
				<td><%= phoneModel.createa_at %></td>
			</tr>
		<% end %>
		</tbody>
	</table>
	<hr> 
	<h2>Customers</h2>
	<table class="bordered-table zebra-striped">
		<thead>
			<tr>
				<th>#</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email Address</th>
				<th>Password</th>
				<th>Created</th>
			</tr>
		</thead>
		<tbody>
			<% for customer in @customers %>
				<tr>
					<th><%= customer.id %></th>
					<th><%= customer.fname %></th>
					<th><%= customer.lname %></th>
					<th><%= customer.email %></th>
					<th><%= customer.passwd %></th>
					<th><%= customer.created_at %></th>
				</tr>
			<% end %>
		</tbody>
	</table
	<hr>
	
		<h6>Models.add</h6>
	
	<form action="/admin" method="post">
		<div class="clearfix">
			Make: <input class="xlarge" id="modelsMake" name="make" type="text" size="30">
		</div>
		<div class="clearfix">
			Model: <input class="xlarge" id="modelsMake" name="model" type="text" size="30">
		</div>
		<div class="clearfix">
			Make: <input class="xlarge" id="modelsMake" name="price" type="text" size="30">
		</div>
		<div class="clearfix">
			Make: <input class="xlarge" id="modelsMake" name="make" type="text" size="30">
		</div>
		<div class="clearfix">
			Make: <textarea class="xxlarge" id="textarea2" name="textarea2" rows="3"></textarea>
		</div>
	</form>
</div>
